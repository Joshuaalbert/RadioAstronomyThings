   "source": [
    "import numpy as np\n",
    "import pylab as plt\n",
    "\n",
    "N = 1000\n",
    "F = np.zeros(N)\n",
    "for i in range(10000):\n",
    "    r = np.random.uniform(size=N)\n",
    "    f = np.fft.fft(np.fft.ifftshift(r))\n",
    "    f[0] = 0\n",
    "    f = np.fft.fftshift(f)\n",
    "    F += np.abs(f)\n",
    "plt.plot(F)\n",
    "plt.show()\n"
   ]
